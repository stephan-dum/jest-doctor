  "devDependencies": {
    "@dev/eslint": "1.0.0",
    "@dev/tsconfig": "1.0.0",
    "@jest/reporters": "30.2.0",
    "@swc/core": "^1.15.8",
    "@swc/jest": "^0.2.39",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/cross-spawn": "^6.0.6",
    "@types/jest": "30.0.0",
    "@types/node": "24.3.3",
    "@types/react": "^19.2.8",
    "@types/react-dom": "^19.2.3",
    "c8": "^10.1.3",
